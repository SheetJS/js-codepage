    "voc":"~1.0.0",
    "exit-on-epipe":"~1.0.1",
    "commander":"~2.11.0"
  },
  "devDependencies": {
    "mocha":"~2.5.3"
  },
  "repository": { "type":"git", "url":"git://github.com/SheetJS/js-codepage.git"},
  "scripts": {
    "pretest": "git submodule init && git submodule update",
